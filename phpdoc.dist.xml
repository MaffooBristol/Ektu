<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Ektu</title>
    <parser>
        <markers>
            <item>@todo</item>
            <item>@fixme</item>
        </markers>
        <target>docs</target>
        <extensions>
            <extension>php</extension>
            <extension>inc</extension>
        </extensions>
    </parser>
    <transformer>
      <target>docs</target>
    </transformer>
    <files>
      <directory>src</directory>
    </files>
</phpdoc>
